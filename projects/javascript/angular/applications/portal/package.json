{
  "name": "@tenlastic/portal-ui",
  "version": "0.0.29",
  "scripts": {
    "build": "ng build --preserve-symlinks --prod portal",
    "docker:build": "sh ../../scripts/docker-build.sh portal portal-ui",
    "docker:push": "sh ../../scripts/docker-push.sh portal-ui",
    "e2e": "ng e2e portal",
    "lint": "ng lint portal",
    "start": "ng serve --port 8081 portal",
    "test": "ng test --code-coverage --watch false portal",
    "test:watch": "ng test portal",
    "version": "../../scripts/version.sh portal-ui"
  },
  "private": true,
  "devDependencies": {
    "@angular/cli": "~8.3.21",
    "@tenlastic/ng-authentication": "0.0.29",
    "@tenlastic/ng-component-library": "0.0.29",
    "@tenlastic/ng-http": "0.0.29"
  }
}
